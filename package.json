{
  "name": "@korbiniankuhn/obj2doc",
  "version": "1.0.0",
  "description": "Parse json or javascript objects to an API documentation format.",
  "main": "index.js",
  "private": false,
  "scripts": {
    "start": "node index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint *.js src/**/*.js test/**/*.js",
    "lint:fix": "eslint *.js src/**/*.js test/**/*.js --fix",
    "precommit": "npm audit && lint-staged && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KorbinianKuhn/obj2doc.git"
  },
  "keywords": [
    "documentation",
    "raml",
    "object",
    "obj2doc",
    "converter",
    "conversion",
    "parse",
    "api"
  ],
  "author": "Korbinian Kuhn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KorbinianKuhn/obj2doc/issues"
  },
  "homepage": "https://github.com/KorbinianKuhn/obj2doc#readme",
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "coveralls": "^3.0.4",
    "eslint": "^6.0.1",
    "husky": "^3.0.0",
    "lint-staged": "^9.0.2",
    "raml2html": "^7.4.0",
    "raml2html-material-theme": "^2.0.1",
    "prettier": "^1.18.2",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "git add"
    ]
  }
}
